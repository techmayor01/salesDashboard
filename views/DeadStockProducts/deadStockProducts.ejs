                    <th class="p-3 border border-gray-200">Products</th>
                    <th class="p-3 border border-gray-200">Model</th>
                    <th class="p-3 border border-gray-200">SKU</th>
                
                    <th class="p-3 border border-gray-200">Quantity</th>
                    <th class="p-3 border border-gray-200">Suppliers Price</th> 
                    <th class="p-3 border border-gray-200">Sells Price</th>
                    <th class="p-3 border border-gray-200">Image</th>
                    <th class="p-3 border border-gray-200">Option</th>
                  </tr>
                </thead>
                <tbody>
                  <tr class="border-t">
                    <td class="p-3 border border-gray-200">1</td>
                    <td class="p-3 border border-gray-200">PCM</td>
                    <td class="p-3 border border-gray-200">Del</td>
                    <td class="p-3 border border-gray-200">Pcm20</td>
                    <td class="p-3 border border-gray-200">231</td>
